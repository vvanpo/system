#!/usr/bin/env python3
import grammar

g = grammar.grammar("""
S = production S
S = newline S
S = production
S = newline
  production = string "=" string newline
  production = string "=" empty-string newline
    string = non-terminal string
    string = terminal string
    string = non-terminal
    string = terminal
    empty-string = 0x3b5
    newline = 0xa newline
    newline = 0xa
""")

print(g)
